{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "no-console":0,
        "max-len": [
            1,
            120,
            2,
            {
                "ignoreComments": true
            }
        ]
    },
    "extends": [
        "eslint:recommended", "plugin:react/recommended"
    ]
}